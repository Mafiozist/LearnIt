<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.VBox?>


<VBox alignment="TOP_CENTER"
      maxHeight="500" maxWidth="500"
      minHeight="50" minWidth="100"
      prefHeight="170.0"
      prefWidth="200.0"
      xmlns="http://javafx.com/javafx/17"
      xmlns:fx="http://javafx.com/fxml/1">

   <children>
      <ImageView id="libraryImage" fitHeight="150.0" fitWidth="200.0" pickOnBounds="true" preserveRatio="true" VBox.vgrow="ALWAYS" />
      <Label id="libraryName" prefHeight="20.0" prefWidth="200.0"
             alignment="CENTER"
             contentDisplay="CENTER"
             text="There is a name of your information"
             textAlignment="CENTER" VBox.vgrow="ALWAYS" />
   </children>

</VBox>
